{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/app.js" }],
  "functions": {
    "app.js": {
      "runtime": "nodejs20.x"
    }
  }
}
